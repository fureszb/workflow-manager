 += para.text + \"\\n\"\n   194→                return text\n   195→            except ImportError:\n   196→                return \"[DOCX fájl - szöveg kinyerés nem támogatott. Telepítse a python-docx csomagot.]\"\n   197→\n   198→        else:\n   199→            return f\"[{file_type.upper()} fájl - szöveg kinyerés nem támogatott ehhez a formátumhoz.]\"\n   200→\n   201→    except Exception as e:\n   202→        return f\"[Hiba a fájl olvasásakor: {str(e)}]\"\n   203→\n   204→\n   205→async def test_ollama_connection(\n   206→    ollama_url: str = DEFAULT_OLLAMA_URL,\n   207→    model: str = DEFAULT_OLLAMA_MODEL,\n   208→) -> dict:\n   209→    \"\"\"Test connection to Ollama server.\n   210→\n   211→    Args:\n   212→        ollama_url: The Ollama server URL\n   213→        model: The model to test with\n   214→\n   215→    Returns:\n   216→        Dict with success status, message, and model info\n   217→    \"\"\"\n   218→    try:\n   219→        async with httpx.AsyncClient(timeout=10.0) as client:\n   220→            # First check if Ollama is running by getting version/tags\n   221→            tags_response = await client.get(f\"{ollama_url}/api/tags\")\n   222→            tags_response.raise_for_status()\n   223→            tags_data = tags_response.json()\n   224→\n   225→            available_models = [m.get(\"name\", \"\") for m in tags_data.get(\"models\", [])]\n   226→\n   227→            # Check if the specified model is available\n   228→            model_available = any(model in m or m in model for m in available_models)\n   229→\n   230→            if not model_available and available_models:\n   231→                return {\n   232→                    \"success\": True,\n   233→                    \"message\": f\"Ollama kapcsolódva, de a '{model}' modell nincs telepítve.\",\n   234→                    \"available_models\": available_models,\n   235→                    \"model_available\": False,\n   236→                }\n   237→\n   238→         